---
title: ""
layout: layout.liquid
---

{% for post in collections.post reversed %}
  <h2 class="tight">
    <a href="{{ post.url }}">{{ post.data.title }}</a>
  </h2>
  <p>
    <strong>{{ post.data.date | postDate }}</strong>
  </p>
  {{ post.content }}
  {% if not forloop.last %}
    <hr/>
  {% endif %}
{% endfor %}